/* 12-07-2016 */
.performance-table{
	display: table;
	width: 100%;
	text-align: center;
}
.performance-table-tr{
	display: table-row;
}
.performance-table-th{
	display: table-cell;
	font-size: 12px;
	color: #434242;
	background-color: #f4f4f4;
	padding: 6px 0;
}
.performance-table-th:first-child{
	text-align: left;
	padding-left: 28px;
}
.performance-table-td{
	display: table-cell;
	font-size: 16px;
	color: #3c3c3c;
	border-bottom: solid 1px #eee;
	padding: 20px 0;
}
.performance-table-td:first-child{
	text-align: left;
	padding-left: 28px;
}
.performance-pic{
	width: 52px;
	height: 52px;
	border: solid 2px #06cc30;
	border-radius: 50%;
	overflow: hidden;
	display: inline-block;
	vertical-align: middle;
	margin-right: 8px;
}
.performance-pic img{
	width: 100%;
	height: auto;
	display: block;
}
.performance-table-td span{
	display: inline-block;
	vertical-align: middle;
	font-size: 15px;
}
.performance-name a{
	font-size: 15px;
	color: #3c3c3c;
	display: inline-block;
}
.performance-name a:hover{
	color: #54c9e8;
}
.performance-table-td span.p-green{
	color: #6bc04b;
}
.performance-table-td span.p-green:after{
	content: '';
    display: inline-block;
    border-style: solid;
    border-width: 7px 5px;
    border-color: transparent transparent #6bc04b transparent;
    position: relative;
    top: -2px;
    margin-left: 5px;
}
.performance-table-td span.p-gray{
	color: #7c7c7c;
}
.performance-table-td span.p-red{
	color: #ff3c1f;
}
.performance-table-td span.p-red:after{
	content: '';
    display: inline-block;
    border-style: solid;
    border-width: 7px 5px;
    border-color: #ff3c1f transparent transparent transparent;
    position: relative;
    top: 5px;
    margin-left: 5px;
}
.performance-table-tr.performance-active .performance-table-td{
	background-color: #fafafa;
}
.performance-table-tr:last-child .performance-table-td{
	border-bottom: 0;
}
.performance-table-td span.p-yellow{
	color: #ff9000;
}
