.conversation.arrow_box {
	position: relative;
	background: #deedd8;
    padding: 15px 20px;
    background: #deedd8;
    color: #4e4c4b;
    position: relative;
    margin-left: 15px;
    border-radius: 4px;
    font-size: 15px;
}
.conversation.arrow_box:after {
	right: 100%;
	top: 30%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
	border-color: rgba(222, 237, 216, 0);
	border-right-color: #deedd8;
	border-width: 10px;
	margin-top: -10px;
}
.chat-convrstaion {
	width: 330px;
     margin-bottom: 60px;
    float: left;
}
.chat-convrstaion span {
    display: block;
    color: #a5a5a5;
    font-size: 12px;
    float: right;
    margin-top: 6px;
}
.chat-convrstaion.chat-rel {
    margin-bottom: 60px;
    float: right;
   
}
.chat-convrstaion.chat-rel .conversation.arrow_box {
    background: #edd5c1;
    margin-left: 0;
    margin-right: 15px;
}
.chat-convrstaion.chat-rel .conversation.arrow_box:after {
    left: 100%;
	top: 30%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
	border-color: rgba(237, 213, 193, 0);
	border-left-color: #edd5c1;
	border-width: 10px;
	margin-top: -10px;
}

